                     Color="White" />
    <SolidColorBrush x:Key="ControlHighlightedBackground"
                     Color="White" />
    <SolidColorBrush x:Key="ControlMouseOverBorder"
                     Color="#2b4060" />
    <SolidColorBrush x:Key="ControlMouseOverBackground"
                     Color="#f8f7fa" />
    <SolidColorBrush x:Key="ControlPressedBorder"
                     Color="#2b4060" />
