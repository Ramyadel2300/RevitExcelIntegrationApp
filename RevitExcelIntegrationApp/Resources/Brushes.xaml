                     Color="#f8f7fa" />
    <SolidColorBrush x:Key="ControlFocusBorder"
                     Color="#2b4060" />
    <SolidColorBrush x:Key="ControlFocusForeground"
                     Color="Black" />
    <SolidColorBrush x:Key="GlyphForeground"
                     Color="#2b4060" />
    <SolidColorBrush x:Key="ListSelectedForeground"
                     Color="#f8f7fa" />
    <SolidColorBrush x:Key="ListSelectedBackground"
                     Color="#2b4060" />
    <SolidColorBrush x:Key="ListSelectedBorder"
                     Color="#ffd491" />
    <SolidColorBrush x:Key="ListMouseOverBackground"
                     Color="#ffd491" />
    <SolidColorBrush x:Key="ListMouseOverBorder"
                     Color="#ffd491" />    
    <SolidColorBrush x:Key="ListFocusedBackground"
                     Color="#2b4060" />
    <SolidColorBrush x:Key="ListFocusedBorder"
                     Color="#ffd491" />
</ResourceDictionary>