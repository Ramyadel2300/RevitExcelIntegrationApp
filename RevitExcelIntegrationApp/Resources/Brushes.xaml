<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="PrimaryBackground"
                     Color="#f8f7fa" />
    <SolidColorBrush x:Key="SecondaryBackground"
                     Color="White" />    
    <SolidColorBrush x:Key="ContentBackground"
                     Color="White" />
    <SolidColorBrush x:Key="PrimaryForeground"
                     Color="Black" />
    <SolidColorBrush x:Key="SecondaryFreground"
                     Color="#2b4060" />
    <SolidColorBrush x:Key="PrimaryBorder"
                     Color="#ffd491" />
